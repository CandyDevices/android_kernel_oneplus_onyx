cat << CTAG
{
	name:WAKE,
		elements:[
			`if [ -f "/proc/touchpanel/sweep_wake_enable" ]; then
				$BB echo '{ SPane:{
					title:"Sweep2Wake",
					description:"This allows you to configure S2W.",
				}},
					{ SButton:{
						label:"Force S2W",
						action:"s2w enable"
					}},
					{ SButton:{
						label:"Unforce S2W",
						action:"s2w disable"
					}},'
			fi`
		]
}
CTAG
